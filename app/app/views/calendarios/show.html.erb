<%- model_class = Calendario -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @calendario.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:InÃ­cio) %>:</strong></dt>
  <dd><%= @calendario.start_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Fim) %>:</strong></dt>
  <dd><%= @calendario.end_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Descricao) %>:</strong></dt>
  <dd><%= @calendario.descricao %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              calendarios_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_calendario_path(@calendario), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              calendario_path(@calendario),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
